<h1>Editing coupon_price</h1>

<% form_for(@coupon_price) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :price %><br />
    <%= f.text_field :price %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @coupon_price %> |
<%= link_to 'Back', coupon_prices_path %>